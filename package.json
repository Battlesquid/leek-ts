{
    "name": "leek",
    "version": "1.0.0",
    "description": "general purpose bot",
    "license": "GPL-3.0-only",
    "main": "dist/index.js",
    "scripts": {
        "tsrun": "npx ts-node -r tsconfig-paths/register src/index.ts",
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
        "build": "yarn clean:build && npx tsc && yarn prisma:generate",
        "clean:build": "rm -rf dist/",
        "clean": "rm -rf node_modules",
        "dev": "yarn build && yarn lint && yarn start",
        "prepare": "husky install",
        "prisma:generate": "npx prisma generate",
        "start": "node dist/index.js"
    },
    "dependencies": {
        "@prisma/client": "4.15.0",
        "@sapphire/framework": "^4.4.3",
        "@sapphire/plugin-subcommands": "^4.0.1",
        "@sapphire/utilities": "^3.11.2",
        "axios": "^0.27.2",
        "discord.js": "14.x",
        "dotenv": "^16.3.0",
        "emoji-regex": "^10.1.0",
        "module-alias": "^2.2.3",
        "ms": "^2.1.3",
        "pg": "^8.7.3",
        "pino": "^7.11.0",
        "pino-abstract-transport": "^0.5.0",
        "prisma": "^4.15.0"
    },
    "devDependencies": {
        "@types/ms": "^0.7.31",
        "@types/node": "^17.0.23",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.6.4"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    },
    "_moduleAliases": {
        "@utils": "dist/utils",
        "@db": "dist/db",
        "@interactions": "dist/interactions"
    }
}