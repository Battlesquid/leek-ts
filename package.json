        "@sapphire/utilities": "^3.16.2",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.31.2",
        "emoji-regex": "^10.3.0",
        "ms": "^2.1.3",
        "node-fetch": "2",
        "pg": "^8.12.0",
        "pino": "^8.20.0",
        "pino-abstract-transport": "^1.1.0",
        "pino-parseable": "^1.0.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
