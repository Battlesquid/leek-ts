{
    "name": "leek",
    "version": "1.0.0",
    "description": "general purpose bot",
    "license": "GPL-3.0-only",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "build": "tsc",
        "build:bot": "yarn build",
        "build:all": "yarn build:clean && yarn build:bot && yarn prisma:generate",
        "build:clean": "yarn clean && yarn build:bot",
        "postinstall": "yarn build:all",
        "dev": "yarn build && yarn start | pino-pretty",
        "dev:ts": "ts-node -r tsconfig-paths/register src/index.ts",
        "lint": "eslint src/**",
        "clean": "rm -rf node_modules",
        "prepare": "husky install",
        "prisma:generate": "prisma generate",
        "start": "node dist/index.js"
    },
    "dependencies": {
        "@prisma/client": "^5.12.1",
        "@sapphire/framework": "^5.1.0",
        "@sapphire/plugin-subcommands": "^6.0.3",
        "@sapphire/utilities": "^3.15.3",
        "axios": "^1.6.8",
        "discord.js": "14.14.1",
        "dotenv": "^16.4.5",
        "emoji-regex": "^10.3.0",
        "module-alias": "^2.2.3",
        "ms": "^2.1.3",
        "pg": "^8.7.3",
        "pino": "^8.20.0",
        "pino-abstract-transport": "^1.1.0",
        "pino-parseable": "^1.0.0",
        "prisma": "^5.12.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@types/ms": "^0.7.34",
        "@types/node": "^20.12.7",
        "@types/pg": "^8.11.5",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "3.2.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.6.0"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.md": "prettier --write"
    },
    "_moduleAliases": {
        "@utils": "dist/utils",
        "@db": "dist/db",
        "@interactions": "dist/interactions",
        "@modals": "dist/modals"
    },
    "volta": {
        "yarn": "1.22.19",
        "node": "21.7.2"
    }
}
